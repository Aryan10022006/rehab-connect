{
  "version": 2,
  "name": "rehab-connect",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_ENVIRONMENT": "production"
  },
  "functions": {
    "build": {
      "memory": 3008,
      "maxDuration": 30
    }
  }
}
